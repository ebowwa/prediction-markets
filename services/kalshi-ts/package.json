{
  "name": "kalshi-ts-service",
  "version": "1.0.0",
  "description": "TypeScript service for Kalshi API - efficient feed handler",
  "type": "module",
  "scripts": {
    "dev": "bun --hot src/index.ts",
    "start": "bun src/index.ts",
    "build": "bun build src/index.ts --outdir ./dist --minify --sourcemap=external",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "test:watch": "bun test --watch"
  },
  "dependencies": {
    "hono": "^4.6.14",
    "kalshi-typescript": "^3.4.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.7.2"
  },
  "trustDependencies": true
}
